<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    <!--  Created by the JSmart -->
<hibernate-mapping>
<class 
    name="com.hod.pojo.Hod2000Heatingparameter" 
    table="hod2000_heatingparameter"
>

    <id
        name="hpId"
        type="java.lang.Integer"
        column="hp_id"
    >
        <generator class="identity" />
    </id>

    <property
        name="hpMonths"
        type="java.lang.Integer"
        column="hp_months"
        length="10"
    />
    <property
        name="hpFactor"
        type="java.lang.Double"
        column="hp_factor"
        length="15"
    />
    <property
        name="hpStart"
        type="java.util.Date"
        column="hp_start"
        length="23"
    />
    <property
        name="hpEnd"
        type="java.util.Date"
        column="hp_end"
        length="23"
    />
	<property
        name="hpType"
        type="java.lang.Integer"
        column="hp_type"
        length="1"
    />
    <!-- Associations -->
  

</class>
</hibernate-mapping>
