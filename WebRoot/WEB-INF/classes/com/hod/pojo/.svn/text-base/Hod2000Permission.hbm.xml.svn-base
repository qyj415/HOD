<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    <!--  Created by the JSmart -->
<hibernate-mapping>
<class 
    name="com.hod.pojo.Hod2000Permission" 
    table="hod2000_permission"
>

    <id
        name="permId"
        type="java.lang.Integer"
        column="perm_id"
    >
        <generator class="identity" />
    </id>

    <property
        name="permName"
        type="java.lang.String"
        column="perm_name"
        length="50"
    />
    <property
        name="permCodeName"
        type="java.lang.String"
        column="perm_code_name"
        length="50"
    />
    <!-- Associations -->
  

</class>
</hibernate-mapping>
