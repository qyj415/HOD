<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    <!--  Created by the JSmart -->
<hibernate-mapping>
<class 
    name="com.hod.pojo.Hod2000DbBackupSchedule" 
    table="hod2000_dbBackupSchedule"
>

    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
        <generator class="identity" />
    </id>

    <property
        name="backupPath"
        type="java.lang.String"
        column="backup_path"
        length="200"
    />
    <property
        name="completePeriod"
        type="java.lang.Integer"
        column="complete_period"
        length="10"
    />
    <property
        name="diffTime"
        type="java.lang.Integer"
        column="diff_time"
        length="10"
    />
    <property
        name="remark"
        type="java.lang.String"
        column="remark"
        length="150"
    />

    <!-- Associations -->
  

</class>
</hibernate-mapping>
