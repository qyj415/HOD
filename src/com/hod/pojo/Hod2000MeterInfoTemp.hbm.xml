        name="meterFlow"
        type="java.lang.String"
        column="meter_flow"
        length="20"
    />
    <property
        name="accumulateTime"
        type="java.lang.Integer"
        column="accumulate_time"
        length="10"
    />
    <property
        name="supplyTemper"
        type="java.lang.String"
        column="supply_temper"
        length="20"
    />
    <property
        name="backTemper"
        type="java.lang.String"
        column="back_temper"
        length="20"
    />
    <property
        name="readTime"
        type="java.util.Date"
