<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    <!--  Created by the JSmart -->
<hibernate-mapping>
<class 
    name="com.hod.pojo.Hod2000PreReceive" 
    table="hod2000_pre_receive"
>

    <id
        name="prId"
        type="java.lang.Integer"
        column="pr_id"
    >
        <generator class="identity" />
    </id>

    <property
        name="roomId"
        type="java.lang.Integer"
        column="room_id"
        not-null="true"
        length="10"
    />
    <property
        name="prMonths"
        type="java.lang.Integer"
        column="pr_months"
        length="10"
    />
    <property
        name="prFactor"
        type="java.lang.Double"
        column="pr_factor"
        length="15"
    />
    <property
        name="prMoney"
        type="java.lang.Double"
        column="pr_money"
        length="15"
    />
    <property
        name="prAfter"
        type="java.lang.Integer"
        column="pr_after"
        length="10"
    />
    <property
        name="prTime"
        type="java.util.Date"
        column="pr_time"
        length="23"
    />
	<property
        name="prOperator"
        type="java.lang.String"
        column="pr_operator"
        length="50"
    />
    <!-- Associations -->
  

</class>
</hibernate-mapping>
